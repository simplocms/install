{
    "name": "simplo-cms/simplo-cms",
    "description": "Simplo CMS",
    "keywords": ["simplo", "cms", "laravel"],
    "license": "MIT",
    "type": "project",
    "version": "2.4.2",
    "require": {
        "php": ">=7.1.3",
        "baum/baum": "^1.1",
        "creativeorange/gravatar": "^1.0",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.7.*",
        "laravelcollective/html": "^5.7",
        "lavary/laravel-menu": "^1.7",
        "nwidart/laravel-modules": "^3.3",
        "pulkitjalan/google-apiclient": "^3.1",
        "sentry/sentry": "^1.9",
        "sentry/sentry-laravel": "^0.10.0",
        "symfony/process": "~4.1",
        "zizaco/entrust": "^1.9",
        "ext-json": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "filp/whoops": "^2.2",
        "fzaninotto/faker": "^1.8",
        "laravel/dusk": "^4.0",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "ide-helper-generate": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "*"
            ]
        }
    }
}
